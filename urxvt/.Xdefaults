URxvt.termName: xterm-256color
URxvt.transparent: true
URxvt.inheritPixmap: False
URxvt.imLocale: en_AU.UTF-8
URxvt.scrollBar: false
URxvt.saveLines: 500
URxvt.urlLauncher:      firefox
URxvt.cursorBlink: true
URxvt.fading: 25%
urxvt.font:             xft:Inconsolata:pixelsize=12
urxvt.boldFont:         xft:Inconsolata:bold:pixelsize=12
URxvt*background:      #303030
urxvt.depth: 32
urxvt.background: rgba:0000/0000/0000/bbbb
URxvt.foreground: grey80
URxvt.tintColor: #262626
URxvt.borderLess: false

